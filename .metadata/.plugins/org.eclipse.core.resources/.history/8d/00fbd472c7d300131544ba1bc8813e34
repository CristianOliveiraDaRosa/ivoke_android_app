package com.app.ivoke.objects;

import java.util.ArrayList;

import com.app.ivoke.helpers.WebHelper;

import android.graphics.Bitmap;

public class MuralPost{
	
	int muralPostId;
	int userId;
    Bitmap profileImg;
    String userName;
	String message;
	String createAt;
    
    public MuralPost( int pMuralPostId
    		        , int pUserId
    		        , String pUserName
    		        , String pMessage
    		        , String pCreateAt
    		        , Bitmap pProfileImage)
    {
    	muralPostId = pMuralPostId;
        userName    = pUserName;
    	message     = pMessage;
    	createAt    = pCreateAt;
    	profileImg  = pProfileImage;
    }
    
    public int getId()
    {
    	return muralPostId;
    }
    
    public String getNome()
    {
    	return userName;
    }
    
    public String getMensagem()
    {
    	return message;
    }
    
    public String getQuando()
    {
    	return createAt;
    }
    
    public Bitmap getProfileImage()
    {
    	return profileImg;
    }
    
    public int getUserId()
    {
    	return userId;
    }
    
}

