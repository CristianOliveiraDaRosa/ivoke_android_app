package com.app.ivoke.objects.defaults;

import android.app.Activity;
import android.os.AsyncTask;

public class DefaultBackgroudWorker extends AsyncTask<Object, Object, Object> {
	
	Activity context;
	
	public DefaultBackgroudWorker(Activity pActivity)
	{
		context = pActivity;
	}
	
	@Override
	abstract protected Object doInBackground(Object... params);
	
	private Activity getActivityCaller()
	{
		return context;
	}

}
