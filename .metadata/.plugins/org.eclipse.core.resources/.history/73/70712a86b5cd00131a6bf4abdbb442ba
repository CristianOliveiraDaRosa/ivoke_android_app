package com.app.ivoke.objects;

import android.graphics.Bitmap;

public class MuralPost{
	
	Bitmap profileImg;
    String userName;
	String mensagem;
	String quando;
    int muralPostId;
    
    public MuralPost(int pMuralPostId, String pUsuarioNome,String pMensagem, String pQuando)
    {
    	userName  = pUsuarioNome;
    	mensagem = pMensagem;
    	quando   = pQuando;
    	muralPostId = pMuralPostId;
    }
    
    public int getId()
    {
    	return muralPostId;
    }
    
    public String getNome()
    {
    	return userName;
    }
    
    public String getMensagem()
    {
    	return mensagem;
    }
    
    public String getQuando()
    {
    	return quando;
    }
    
    public Bitmap getProfileImage()
    {
    	return profileImg;
    }
    
}

